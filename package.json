{
  "name": "eventscalendar",
  "version": "1.1.0",
  "description": "Dynamically loaded events via json",
  "main": "src/js/App.js",
  "scripts": {
    "build": "npm run format && parcel build src/js/App.js --out-dir build --out-file App.js",
    "dev": "parcel src/index.htm",
    "format": "npm run lint && npm run prettier",
    "prettier": "prettier --write \"src/**/*.{js,jsx,css,htm,json}\"",
    "prettier:check": "prettier --list-different \"src/**/*.{js,jsx,css,htm,json}\"",
    "lint": "eslint \"src/js/**/*.{js,jsx}\""
  },
  "author": "Tim Gorsuch",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "sass": "^1.23.0-module.beta.1"
  },
  "dependencies": {
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3"
  }
}
